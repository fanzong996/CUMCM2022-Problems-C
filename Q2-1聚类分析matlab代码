X=X_PbBa;

BX=zscore(X); % 标准化数据矩阵

Y=pdist(X) % 用欧氏距离计算两两之间的距离

D=squareform(Y) % 欧氏距离矩阵

Z = linkage(Y) % 最短距离法

T = cluster(Z,3) %等价于 { T=clusterdata(X,3) }

find(T==3) % 第3类集合中的元素
%cutoff = median([Z(end-2,3) Z(end-1,3)]);	% 让其最小距离在倒数第二三行中，其取倒数第二行，即分成三类
[H,T]=dendrogram(Z,0) % 画聚类图
